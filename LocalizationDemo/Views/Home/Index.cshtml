@using LocalizationDemo.Data
@using Microsoft.Extensions.Localization
@model List<Foo>
@inject IStringLocalizer text
@{
    ViewData["Title"] = "Index";
}

<h2>@text["some-key"]</h2>

@foreach (Foo foo in Model)
{
    <div>
        <h2>Foo @foo.Id</h2>
        <ul>
            @foreach (Bar bar in foo.Bars)
            {
                <li>@bar.Value</li>
            }
        </ul>
    </div>
}

